{
  "name": "omc-agent-tui",
  "version": "0.1.0-beta",
  "description": "Real-time TUI dashboard for monitoring AI agent orchestration events",
  "author": "chamdom",
  "license": "MIT",
  "main": "cmd/omc-tui/main.go",
  "build": {
    "command": "go build -o bin/omc-tui ./cmd/omc-tui",
    "output": "bin/omc-tui"
  },
  "run": {
    "command": "bin/omc-tui",
    "args": {
      "watch": {
        "description": "Directory to watch for JSONL event files",
        "type": "string",
        "required": false
      },
      "replay": {
        "description": "JSONL file to replay",
        "type": "string",
        "required": false
      }
    }
  },
  "requirements": {
    "go": ">=1.23"
  },
  "keywords": ["tui", "agent", "monitoring", "dashboard", "bubbletea"]
}
